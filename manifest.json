{
  "lang": "en",
  "default_locale": "en",
  "version":"2.0 beta",
  "manifest_version": "2",
  "dir": "ltr",
  "name": "Academy High School Scheduler",
  "description": "Generates your schedule for Plano Academy High School",
  "short_name": "Scheduler",
  "display": "standalone",
  "icons": [{
    "src": "favicon.ico",
    "sizes": "16x16 32x32 64x64",
    "type": "image/ico"
  },{
    "src": "favicon_hires.png",
    "sizes": "128x128 256x256 512x512",
    "type": "image/png"
  }],
  "start_url": "./index.html",
  "homepage_url": "https://github.com/liavt/Scheduler",
  "display": "fullscreen",
  "orientation": "portrait",
  "theme_color": "#9999FF",
  "background_color": "#AAAAFF",
  "screenshots": [{
    "src": "demos/DemoStudent.PNG",
    "sizes": "1662x921",
    "type": "image/png"
  }],
  "permissions": [
    "cookies",
    "notifications"
  ]
}